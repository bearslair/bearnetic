{
	"name": "bearnetic",
	"version": "1.0.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/bearslair/bearnetic.git"
	},
	"scripts": {
		"dev": "concurrently -c green.bold,blue.bold 'npm:dev:docker' 'npm:dev:next'",
		"dev:next": "next dev",
		"dev:docker": "docker compose up",
		"dev:codegen": "npm run codegen:graphql -- --watch",
		"prisma:docker": "npm run dev:docker && migrate && generate",
		"migrate": "prisma migrate dev",
		"generate": "prisma generate",
		"codegen": "npm run codegen:schema && npm run codegen:graphql",
		"codegen:schema": "dotenv -- ts-node --transpile-only -r tsconfig-paths/register ./src/graphql/index.ts",
		"codegen:graphql": "graphql-codegen"
	},
	"dependencies": {
		"@apollo/client": "^3.4.11",
		"@headlessui/react": "^1.6.4",
		"@hookform/resolvers": "^2.8.0",
		"@next/bundle-analyzer": "^12.0.10",
		"@pothos/core": "^3.1.1",
		"@pothos/plugin-prisma": "^0.16.1",
		"@pothos/plugin-scope-auth": "^3.0.0",
		"@pothos/plugin-simple-objects": "^3.0.0",
		"@pothos/plugin-validation": "^3.0.0",
		"@prisma/client": "3.9.1",
		"@types/node": "^17.0.42",
		"class-variance-authority": "^0.2.2",
		"clsx": "^1.1.1",
		"date-fns": "^2.23.0",
		"graphql": "^16.3.0",
		"graphql-helix": "^1.7.0",
		"hash-wasm": "^4.9.0",
		"install": "0.13.0",
		"iron-session": "^6.0.5",
		"jsonwebtoken": "^8.5.1",
		"next": "^12.0.10",
		"nprogress": "^0.2.0",
		"phosphor-react": "^1.4.1",
		"postmark": "^2.8.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hook-form": "^7.25.3",
		"sb": "^6.5.9",
		"zod": "^3.8.2"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.4.0",
		"@graphql-codegen/near-operation-file-preset": "^2.2.3",
		"@graphql-codegen/typescript": "^2.4.2",
		"@graphql-codegen/typescript-operations": "^2.2.3",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/typography": "^0.5.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"autoprefixer": "10.4.5",
		"concurrently": "^7.0.0",
		"dotenv": "^16.0.0",
		"dotenv-cli": "^4.1.1",
		"eslint": "^8.8.0",
		"eslint-config-next": "^12.0.10",
		"husky": "^7.0.2",
		"lint-staged": "^12.3.3",
		"postcss": "^8.4.6",
		"prettier": "^2.4.0",
		"prisma": "3.9.1",
		"tailwindcss": "^3.0.18",
		"trash-cli": "^5.0.0",
		"ts-node": "^10.2.1",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^4.4.3"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"engines": {
		"node": ">=14"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"maintained node versions"
	],
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
}
